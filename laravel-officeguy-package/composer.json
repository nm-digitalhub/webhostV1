{
    "name": "nm-digitalhub/laravel-officeguy",
    "description": "Laravel package for SUMIT payment gateway integration with invoice creation, credit card token storage, and secure transaction processing",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "payment",
        "gateway",
        "sumit",
        "officeguy",
        "invoice",
        "credit-card"
    ],
    "authors": [
        {
            "name": "NM Digital Hub",
            "email": "info@nm-digitalhub.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0",
        "illuminate/events": "^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^10.0|^11.0",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "NmDigitalHub\\LaravelOfficeGuy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NmDigitalHub\\LaravelOfficeGuy\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NmDigitalHub\\LaravelOfficeGuy\\OfficeGuyServiceProvider"
            ],
            "aliases": {
                "OfficeGuy": "NmDigitalHub\\LaravelOfficeGuy\\Facades\\OfficeGuy"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
