{
    "name": "sumit/laravel-payment-gateway",
    "description": "SUMIT Payment Gateway for Laravel - Accept credit card payments with invoice generation and secure token storage",
    "keywords": [
        "laravel",
        "payment",
        "gateway",
        "sumit",
        "credit-card",
        "invoice",
        "recurring-billing"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "SUMIT",
            "email": "support@sumit.co.il"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "laravel/framework": "^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0",
        "spatie/laravel-settings": "^3.0",
        "filament/filament": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Sumit\\LaravelPayment\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sumit\\LaravelPayment\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sumit\\LaravelPayment\\SumitPaymentServiceProvider"
            ],
            "aliases": {
                "SumitPayment": "Sumit\\LaravelPayment\\Facades\\SumitPayment"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
